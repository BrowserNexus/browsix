{
  "name": "browsix",
  "version": "1.0.0",
  "description": "in-browser, multi-process operating system",
  "main": "lib/kernel/kernel.js",
  "scripts": {
    "needs-format": "prettier -l \"src/**/*.ts\" \"test/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/*.ts\"",
    "lint": "yarn run tslint -c tslint.json --project tsconfig.json",
    "build:pre": "node support/mkdirp.js lib-dist/kernel lib/src/kernel",
    "build:kernel": "tsc -p tsconfig.json",
    "build:bin": "node support/build-bin.js",
    "build": "npm-run-all build:pre build:kernel build:bin",
    "prepublishOnly": "make clean && npm run-script build"
  },
  "repository": {
    "url": "https://github.com/plasma-umass/browsix.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plasma-umass/browsix/issues"
  },
  "homepage": "https://browsix.org",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/filesystem": "0.0.29",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.22",
    "browser-sync": "^2.26.7",
    "browserfs-browsix-tmp": "1.9.5",
    "chai": "^4.1.2",
    "karma": "^5.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-mocha": "^2.0.1",
    "mocha": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "tslint": "^6.1.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "node-binary-marshal": "^0.4.2"
  },
  "engines": {
    "node": ">=12"
  }
}
